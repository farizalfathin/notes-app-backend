{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "eol-last": "off",
        "no-underscore-dangle": "off",
        "camelcase": "off",
        "import/no-extraneous-dependencies": "off"
    }
}
